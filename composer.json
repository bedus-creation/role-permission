{
    "name": "aammui/role-permission",
    "description": "Role permission system for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Bedram Tamang",
            "email": "tmgbedu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/auth": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.40|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Aammui\\RolePermission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aammui\\RolePermission\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^9.0",
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "vimeo/psalm": "^4.7"
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aammui\\RolePermission\\RolePermissionServiceProvider"
            ]
        }
    }
}
